      "\\n"\
     ]\
    \}\
   ],\
   "source": [\
    "shuffle_small_dataset = TtyrecDataset(\\n",\
    "    \\"autoascend\\",\\n",\
    "    batch_size=2,\\n",\
    "    seq_length=30000,\\n",\
    "    dbfilename=database_name,\\n",\
    "    shuffle=True,\\n",\
